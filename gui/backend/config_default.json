{
    "module_source_youtube" : {   
        "output_queue" : "stage1",
        "stream_queue_limit" : 1000,
        "params": {
            "url": "https://www.youtube.com/watch?v=pQKhS4F9sSM",
            "stream": "normal:mp4@640x360",
            "stream_": "normal:mp4@1920x1080"
        }
    },
    "module_source_cv" : {   
        "output_queue" : "stage1",
        "stream_queue_limit" : 100,
        "params": {
            "device": 1
        }
    },
    "module_source_cv" : {   
        "output_queue" : "stage1",    
        "stream_queue_limit" : 100,
        "params": {
            "expire_time" : 600,
            "device" : "../../samples/youtube/out_2_21.mp4"
        }
    },
    "module_source_dir" : {   
        "output_queue" : "stage1",  
        "stream_queue_limit" : 100,  
        "params": {
            "path": "../../samples/youtube"
        }
    },

    "module_zbuffer" : {
        "input_queue" : "stage1",
        "stream_queue_limit" : 100,
        "output_queue" : "stage2",
        "params" : {
            "wls_lambda": 8000,
            "wls_sigma": 1.5,
            "buffer_size": 2
        }
    },
    "module_motion_detection" : { 
        "input_queue" : "stage1",    
        "stream_queue_limit" : 100,         
        "output_queue" : "stage2",  
        "params": {
            "buffer_size": 25,
            "pixel_treshold": 5000,
            "frame_gap": 5
        }
    },

    
    "module_sink_win" : {   
        "input_queue" : "stage1",
        "params": {
            "window_name": "camera_view"
        }        
    },
    "module_sink_win" : {   
        "input_queue" : "stage1",    
        "input_streams" : {
            "color" : "stream_video",
            "metrics" : "stream_data"
        },
        "params": {
            "window_name": "camera_view"
        }        
    },
    "module_sink_file" : {   
        "input_queue" : "stage1",    
        "params": {
            "filename": "out/test_%Y%m%d_%H%M%S_%f.avi",
            "framerate": 25,
            "fourcc": "XVID"
        }        
    }
}